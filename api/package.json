{
  "name": "@lambda_terraform/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "webpack": "webpack",
    "start-server": "node ./build/server.js",
    "start:watch": "aws-vault exec personal-free-3 nodemon",
    "build": "tsc --build",
    "watch": "yarn build --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@koa/router": "^8.0.8",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/koa__router": "^8.0.2",
    "aws-sdk": "^2.670.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "moment": "^2.24.0",
    "nodemon": "^2.0.3",
    "serverless-http": "^2.3.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "./build",
      "../data",
      "../core-backend"
    ],
    "legacyWatch": true,
    "delay": "1000",
    "exec": "yarn start-server"
  }
}
